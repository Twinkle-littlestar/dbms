mysql> create table Student_info(Rollno int primary key,Name varchar(20),Address varchar(50));
Query OK, 0 rows affected (0.04 sec)

mysql> insert into Student_info(Rollno,Name,Address) values(1,'Ravi','Nashik'),(2,'Vedika','Nashik'),(5,'Tushar','Pune'),(6,'Varsha','Mumbai');
Query OK, 4 rows affected (0.01 sec)
Records: 4  Duplicates: 0  Warnings: 0

mysql> select*from Student_info;
+--------+--------+---------+
| Rollno | Name   | Address |
+--------+--------+---------+
|      1 | Ravi   | Nashik  |
|      2 | Vedika | Nashik  |
|      5 | Tushar | Pune    |
|      6 | Varsha | Mumbai  |
+--------+--------+---------+
4 rows in set (0.00 sec)

mysql> select Student_Marks.Rollno,Student_Marks.Marks,Student_info.Address
    -> from Student_Marks inner join Student_info
    -> On Student_Marks.Rollno=Student_info.Rollno;
+--------+-------+---------+
| Rollno | Marks | Address |
+--------+-------+---------+
|      1 |    90 | Nashik  |
|      2 |    70 | Nashik  |
+--------+-------+---------+
2 rows in set (0.01 sec)

mysql> select Student_Marks.Rollno,Student_Marks.Marks,Student_info.Address
    -> from Student_info right join Student_Marks
    -> on Student_info.Rollno=Student_Marks.Rollno;
